import java.util.*;

public class createvm {
	public static void main(String[] args) {
		vmServer a = new vmServer();
	}
	
	public class vmServer{
		List<Vm> a = null;
		public vmServer(){
			a = new ArrayList<Vm>();
		}
		
		public void add(Vm e){
			a.add(e);
		}
		
		public int get(){
			return a.size();
		}
	}
	
	public class Vm{
		private int i =0;	
		private Vm(int a){
			this.i = a;
		}
	}
}
